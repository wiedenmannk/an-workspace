{
  "name": "an-workspace",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve -o --project=skeleton",
    "build": "ng build",
    "build:lib": "ng build --project=starter-lib && npm run scssbuild",
    "watch": "ng build --watch --configuration development",
    "watch:lib": "ng build --watch --project=starter-lib",
    "test": "ng test",
    "scssbuild": "sass projects/starter-lib/src/lib/styles:dist/starter-lib/styles"
  },
  "private": true,
  "dependencies": {
    "@angular-eslint/eslint-plugin": "^15.0.0",
    "@angular-eslint/eslint-plugin-template": "^15.0.0",
    "@angular-eslint/template-parser": "^15.1.0",
    "@angular/animations": "^14.1.0",
    "@angular/common": "^14.1.0",
    "@angular/compiler": "^14.1.0",
    "@angular/core": "^14.1.0",
    "@angular/forms": "^14.1.0",
    "@angular/platform-browser": "^14.1.0",
    "@angular/platform-browser-dynamic": "^14.1.0",
    "@angular/router": "^14.1.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "prettier-eslint": "^15.0.1",
    "primeng": "^14.0.2",
